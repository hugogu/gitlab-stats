          },
          "measure": "additions",
          "rawQuery": "WITH all_hours AS (\n  SELECT h FROM UNNEST(SEQUENCE(0, 23)) as t(h)\n)\nSELECT all_hours.h, CREATE_TIME_SERIES(data.date, value) hour\nFROM all_hours\nLEFT JOIN (\n  SELECT BIN(time, $period) date, (hour(time) + 8) % 24 h, count(*) AS value\n  FROM \"gitlab-stat\".\"gitlab-history\" \n  WHERE (time BETWEEN from_milliseconds($__timeFrom) AND from_milliseconds($__timeTo))\n     AND author IN ($author) AND author != 'autogit'\n     AND \"group\" IN ($group)\n     AND measure_name IN ('additions')\n    AND parents = '1'\n    AND project in ($project)\n  GROUP BY hour(time), BIN(time, $period)\n) AS data ON all_hours.h = data.h\nGROUP BY all_hours.h\nORDER BY all_hours.h",
          "refId": "A",
          "table": "\"gitlab-history\""
        }
      ],
      "title": "Commit Distribution During a Day",
